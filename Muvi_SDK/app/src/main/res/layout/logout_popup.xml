<?xml version="1.0" encoding="utf-8" ?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_result3"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/button_radious"
    android:orientation="vertical"
    android:paddingBottom="@dimen/voucher_pop_up_padding">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/app_background"
        android:orientation="vertical"
        android:padding="@dimen/Login_drawing_padding">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_marginBottom="@dimen/loginImageViewMarging_top"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/logout_text"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/logouttext_height"
                android:gravity="center"
                android:paddingLeft="@dimen/Login_drawing_padding"
                android:paddingRight="@dimen/Login_drawing_padding"
                android:text="You have crossed your maximum login limit.Please logout all the devices ."
                android:textColor="@color/pageTitleColor"
                android:textSize="@dimen/logout_text_size"
                android:textStyle="normal" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/videoGenreTextView_padding"
            android:paddingTop="@dimen/login_new_user_padding">

            <Button
                android:id="@+id/ok"
                android:layout_width="@dimen/okbtn_width"
                android:layout_height="@dimen/okbtn_height"
                android:layout_marginLeft="@dimen/textColor_margin"
                android:layout_marginRight="@dimen/ppv_margin"
                android:background="@drawable/button_radious"
                android:text="Ok"
                android:textAllCaps="false"
                android:textColor="@color/buttonTextColor"
                android:textSize="@dimen/okbtn_text_size"
                android:textStyle="bold" />

            <Button
                android:id="@+id/cancel"
                android:layout_width="@dimen/okbtn_width"
                android:layout_height="@dimen/okbtn_height"
                android:layout_gravity="right"
                android:layout_marginLeft="@dimen/ppv_margin"
                android:background="@drawable/button_radious"
                android:text="Cancel"
                android:textAllCaps="false"
                android:textColor="@color/buttonTextColor"
                android:textSize="@dimen/okbtn_text_size" />

        </LinearLayout>

    </LinearLayout>

</LinearLayout>
